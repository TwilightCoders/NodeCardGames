  crossorigin="anonymous"></script>
<!-- bootbox -->
<script type="text/javascript" src="/scripts/bootbox.min.js"></script>

<% if (threeThirteen.js) { // Check to see if threeThirteen.js has been set, and if so, use that to pull the correct local JS file %>
<!-- app's js -->
<script type="text/javascript" src="/scripts/threeThirteen/<%= threeThirteen.js %>"></script>
<% } %>


<% if (typeof game !== "undefined") { %>
<!-- game settings, to be used within the app game object -->
<script>
$(document).ready(function()
{
  // Initial game data (will be updated by ajax)
  game.data.shortId = "<%- game.shortId %>";

  // Get the game data, then 
  game.init();
});
</script>
<% } %>

</body>
</html>